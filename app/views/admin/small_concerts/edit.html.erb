<%= form_for [:admin ,@small_concert] do |f| %>
  <% if @small_concert.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@small_concert.errors.count, "error") %> prohibited this small_concert from being saved:</h2>

      <ul>
      <% @small_concert.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    年<br>
    <%= f.number_field :year %>
  </div>
  <div class="field">
    月<br>
    <%= f.number_field :month %>
  </div>
  <div class="field">
    編集受付<br>
    <%= f.check_box :can_edit %>
  </div>
  <div class="actions">
    <input name="commit" type="submit" value="上書き">
  </div>
<% end %>
<%= link_to 'Back', '/admin/small_concerts' %>
